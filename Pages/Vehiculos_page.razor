@page "/vehiculos"
@inject VehiculoService VehiculoService
@using BlazorApp1.Shared.Componentes
@using BlazorApp1.Models


<BlazorApp1.Shared.Componentes.TablaVehiculos Vehiculos="vehiculos">

</BlazorApp1.Shared.Componentes.TablaVehiculos>


@code {

    private List<Vehiculo> vehiculos = new List<Vehiculo>();

    protected override async Task OnInitializedAsync()
    {
        vehiculos = await VehiculoService.GetVehiculosAsync();
    }

}

